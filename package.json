    "test-automation",
    "ai",
    "openai",
    "claude",
    "gemini",
    "multi-agent",
    "genetic-algorithm"
  ],
  "author": "QA Engineering Team",
  "license": "PROPRIETARY",
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.2.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "private": true
